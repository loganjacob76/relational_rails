<h1>All Employees</h1>

<% @employees.each do |employee| %>
  <h2><%= employee.name %></h2>
  <p>Age: <%= employee.age %></p>
  <p>Store Employed At: <%= employee.employed_at %></p>
  <p>Employee of the Month: <%= employee.employee_of_the_month %></p>
<% end %>
